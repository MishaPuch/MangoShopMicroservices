{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Ocelot": "Debug"
    }
  },
  "AllowedHosts": "*",
  "FileCacheOptions": {
    "TtlSeconds": 15,
    "Region": "europe-central",
    "Header": "OC-Caching-Control",
    "EnableContentHashing": false
  }
}
